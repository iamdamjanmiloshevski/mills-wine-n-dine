{
  "name": "mills-wine-n-dine",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "^0.29.0",
    "mongodb": "^3.6.4",
    "mongoose": "^5.11.16",
    "mongoose-currency": "^0.2.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "eslint": "^7.20.0"
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ]
    }
  }
}
